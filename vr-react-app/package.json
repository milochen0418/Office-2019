{
  "name": "react-vr-hello",
  "version": "1.0.0",
  "description": "react-vr helloworld project",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node -e \"console.log('open browser at http://localhost:8081/vr/\\n\\n');\" && node node_modules/react-native/local-cli/cli.js start",
    "bundle": "node node_modules/react-vr/scripts/bundle.js",
    "open": "node -e \"require('xopen')('http://localhost:8081/vr/')\"",
    "devtools": "react-devtools",
    "test": "jest",
    "help": "cat ./package.json | jq '.scripts'"
  },
  "dependencies": {
    "ovrui": "~1.4.0",
    "react": "~15.4.1",
    "react-native": "~0.42.0",
    "three": "^0.80.1",
    "react-vr": "~1.4.0",
    "react-vr-web": "~1.4.0"
  },
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-preset-react-native": "^1.9.1",
    "jest": "^19.0.2",
    "react-devtools": "^2.1.3",
    "react-test-renderer": "~15.4.1",
    "xopen": "1.0.0"
  },
  "jest": {
    "preset": "react-vr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milochen0418/react-vr-hello.git"
  },
  "keywords": [
    "react",
    "vr",
    "project",
    "hello",
    "world"
  ],
